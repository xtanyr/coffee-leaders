{
  "name": "coffee-leaders",
  "version": "1.0.0",
  "description": "Coffee Shop Leaders Monitoring Tool",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "ts-node-dev --esm --respawn --transpile-only src/index.ts",
    "migrate": "ts-node --esm src/config/migrate.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
